{
  "name": "isq/maio",
  "license": "MIT",
  "description": "A package to merge arrays into objects in PHP.",
  "autoload": {
    "psr-4": {
      "MAIO\\Attributes\\": "src/Attributes/",
      "MAIO\\Exceptions\\": "src/Exceptions/",
      "MAIO\\": "src/MergeArrayIntoObject.php"
    }
  },
  "require": {
    "php": "^8.4",
    "illuminate/support": "^12.20"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.2",
    "larapack/dd": "^1.1"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
