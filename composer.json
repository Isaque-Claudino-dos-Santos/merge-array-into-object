{
  "name": "isq/maio",
  "license": "MIT",
  "description": "A package to merge arrays into objects in PHP.",
  "readme": "README.md",
  "keywords": [
    "merge",
    "object",
    "array",
    "dto",
    "auto",
    "Merge Array Into Object",
    "maio"
  ],
  "support": {
    "issues": "https://github.com/Isaque-Claudino-dos-Santos/merge-array-into-object/issues",
    "source": "https://github.com/Isaque-Claudino-dos-Santos/merge-array-into-object"
  },
  "authors": [
    {
      "name": "Isaque Claudino dos Santos",
      "email": "isaqueclaudino12@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "ISQ\\MAIO\\": "src/"
    },
    "files": [
      "src/helpers/string-helpers.php",
      "src/helpers/array-helpers.php",
      "src/helpers/benchmark-helpers.php",
      "src/helpers/enum-helpers.php"
    ]
  },
  "require": {
    "php": ">=8"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.86",
    "larapack/dd": "^1.1",
    "phpunit/phpunit": "^12.2"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "./vendor/bin/phpunit ./tests",
    "format": "./vendor/bin/php-cs-fixer fix --config .php-cs-fixer.dist.php src tests"
  }
}
